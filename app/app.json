{
  "expo": {
    "name": "CodeGalaxyApp",
    "entryPoint": "./App.dev.js",
    "slug": "codegalaxy-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Mobile app for CodeGalaxy.io, written in Scala.js",
    "githubUrl": "https://github.com/viktor-podzigun/codegalaxy-app"
  }
}
